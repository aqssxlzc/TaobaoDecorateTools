{
 "manifest_version": 2,
  "name": "TaobaoTools",
  "version": "0.0.1",
   "content_scripts": [
        {
            "matches": [
                "*://*.baidu.com/*"
            ],
            "js": [
                "myscript.js"
            ],
            "run_at": "document_end"
        },
        {
            "matches": [
                "*://*.baidu.com/*"
            ],
            "js": [
                "anotherscript.js"
            ],
            "all_frames": true
        }
    ],
    "permissions": [
        "<all_urls>"
    ]
}